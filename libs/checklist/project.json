{
  "projectType": "library",
  "sourceRoot": "libs/checklist/src",
  "prefix": "ngconf",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/checklist"
      ],
      "options": {
        "jestConfig": "libs/checklist/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/checklist/**/*.ts",
          "libs/checklist/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
